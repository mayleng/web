    </client>
  </system.serviceModel>
  <applicationSettings>
    <TestWebService.Properties.Settings>
      <setting name="TestWebService_MyWebReference_WebService1" serializeAs="String">
        <value>http://localhost/WebApplication2/WebService1.asmx</value>
      </setting>
    </TestWebService.Properties.Settings>
  </applicationSettings>
